      "types": "./dist/index.d.ts",
      "default": "./dist/image-filter.umd.cjs"
    }
  },
  "files": [
    "dist/*.css",
    "dist/*.js",
    "dist/*.cjs",
    "dist/*.d.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "https://github.com/tony-xlh/image-filter",
  "author": "xulihang <xulihanghai@gmail.com> (https://github.com/xulihang)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tony-xlh/image-filter/issues"
  },
  "homepage": "https://github.com/tony-xlh/image-filter#readme",
  "keywords": [
    "document",
    "scanning",
    "correction",
    "enhance",
    "image",
    "filter",
    "retouch",
    "dynamsoft"
  ],
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3"
  },
  "dependencies": {
    "otsu": "^0.0.5"
  }
