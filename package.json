{
  "name": "image-filter-js",
  "private": false,
  "version": "0.1.2",
  "type": "module",
  "main": "./dist/image-filter.umd.cjs",
  "module": "./dist/image-filter.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/image-filter.js"
